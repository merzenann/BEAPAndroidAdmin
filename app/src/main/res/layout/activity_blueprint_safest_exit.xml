<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.merzensumagaysay.beapandroidadmin.BlueprintSafestExit">


    <ImageView
        android:id="@+id/imageView2nd"
        android:layout_width="318dp"
        android:layout_height="230dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/blueprint"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.077" />

    <CheckBox
        android:id="@+id/MFCExit2nd"
        android:clickable="false"
        android:layout_width="36dp"
        android:layout_height="34dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:checked="false"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2nd"
        app:layout_constraintEnd_toEndOf="@+id/imageView2nd"
        app:layout_constraintHorizontal_bias="0.251"
        app:layout_constraintStart_toStartOf="@+id/imageView2nd"
        app:layout_constraintTop_toTopOf="@+id/imageView2nd"
        app:layout_constraintVertical_bias="0.122" />

    <CheckBox
        android:id="@+id/backgateExit2nd"
        android:clickable="false"
        android:layout_width="34dp"
        android:layout_height="31dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:checked="false"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2nd"
        app:layout_constraintEnd_toEndOf="@+id/imageView2nd"
        app:layout_constraintHorizontal_bias="0.716"
        app:layout_constraintStart_toStartOf="@+id/imageView2nd"
        app:layout_constraintTop_toTopOf="@+id/imageView2nd"
        app:layout_constraintVertical_bias="0.136" />

    <CheckBox
        android:id="@+id/mainExit2nd"
        android:clickable="false"
        android:layout_width="32dp"
        android:layout_height="30dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2nd"
        app:layout_constraintEnd_toEndOf="@+id/imageView2nd"
        app:layout_constraintStart_toStartOf="@+id/imageView2nd"
        app:layout_constraintTop_toTopOf="@+id/imageView2nd"
        app:layout_constraintVertical_bias="0.869" />

    <CheckBox
        android:id="@+id/mainGateExit2nd"
        android:clickable="false"
        android:layout_width="31dp"
        android:layout_height="29dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2nd"
        app:layout_constraintEnd_toEndOf="@+id/imageView2nd"
        app:layout_constraintHorizontal_bias="0.638"
        app:layout_constraintStart_toStartOf="@+id/imageView2nd"
        app:layout_constraintTop_toTopOf="@+id/imageView2nd"
        app:layout_constraintVertical_bias="0.864" />

    <CheckBox
        android:id="@+id/LRTExit2nd"
        android:clickable="false"
        android:layout_width="32dp"
        android:layout_height="28dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2nd"
        app:layout_constraintEnd_toEndOf="@+id/imageView2nd"
        app:layout_constraintHorizontal_bias="0.955"
        app:layout_constraintStart_toStartOf="@+id/imageView2nd"
        app:layout_constraintTop_toTopOf="@+id/imageView2nd"
        app:layout_constraintVertical_bias="0.86" />

    <ProgressBar
        android:id="@+id/myProgressBar"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:indeterminate="true"
        android:indeterminateBehavior="cycle"
        android:visibility="gone" />

    <ListView
        android:id="@+id/mListView"
        android:layout_weight="0.5"
        android:numColumns="auto_fit"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</android.support.constraint.ConstraintLayout>
